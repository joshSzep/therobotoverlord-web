  const loadRelatedTopics = async () => {
    /* istanbul ignore next */
    cov_1c47ji5rd2().f[6]++;
    cov_1c47ji5rd2().s[39]++;
    try {
      /* istanbul ignore next */
      cov_1c47ji5rd2().s[40]++;
