        /* istanbul ignore next */
        (cov_1c47ji5rd2().s[44]++, response.data.filter(topic => {
          /* istanbul ignore next */
          cov_1c47ji5rd2().f[7]++;
          cov_1c47ji5rd2().s[45]++;
          return topic.slug !== currentTopicSlug;
