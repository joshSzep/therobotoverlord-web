      /* istanbul ignore next */
      cov_1c47ji5rd2().s[49]++;
      console.error('Failed to load related topics:', err);
    } finally {
      /* istanbul ignore next */
      cov_1c47ji5rd2().s[50]++;
      setIsLoading(false);
    }
  };
  // Initial load
  /* istanbul ignore next */
  cov_1c47ji5rd2().s[51]++;
  (0, _react.useEffect)(() => {
    /* istanbul ignore next */
    cov_1c47ji5rd2().f[8]++;
    cov_1c47ji5rd2().s[52]++;
