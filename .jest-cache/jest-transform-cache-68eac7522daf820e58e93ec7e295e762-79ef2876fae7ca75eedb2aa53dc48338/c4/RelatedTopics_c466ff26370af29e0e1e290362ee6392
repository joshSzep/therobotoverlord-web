      cov_1c47ji5rd2().s[28]++;
      if (
      /* istanbul ignore next */
      (cov_1c47ji5rd2().b[15][0]++, desc) && (
      /* istanbul ignore next */
      (cov_1c47ji5rd2().b[15][1]++, desc.get) ||
      /* istanbul ignore next */
      (cov_1c47ji5rd2().b[15][2]++, desc.set))) {
        /* istanbul ignore next */
        cov_1c47ji5rd2().b[14][0]++;
        cov_1c47ji5rd2().s[29]++;
        Object.defineProperty(newObj, key, desc);
      } else {
        /* istanbul ignore next */
        cov_1c47ji5rd2().b[14][1]++;
        cov_1c47ji5rd2().s[30]++;
        newObj[key] = obj[key];
      }
    } else
    /* istanbul ignore next */
    {
      cov_1c47ji5rd2().b[11][1]++;
    }
  }
  /* istanbul ignore next */
  cov_1c47ji5rd2().s[31]++;
  newObj.default = obj;
  /* istanbul ignore next */
  cov_1c47ji5rd2().s[32]++;
  if (cache) {
    /* istanbul ignore next */
    cov_1c47ji5rd2().b[16][0]++;
    cov_1c47ji5rd2().s[33]++;
    cache.set(obj, newObj);
  } else
  /* istanbul ignore next */
  {
    cov_1c47ji5rd2().b[16][1]++;
  }
  cov_1c47ji5rd2().s[34]++;
  return newObj;
}
function RelatedTopics({
  topicId,
  currentTopicSlug,
  limit =
  /* istanbul ignore next */
  (cov_1c47ji5rd2().b[17][0]++, 5),
  className =
  /* istanbul ignore next */
  (cov_1c47ji5rd2().b[18][0]++, '')
}) {
  /* istanbul ignore next */
  cov_1c47ji5rd2().f[5]++;
