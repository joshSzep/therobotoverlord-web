      (cov_lh93w0ojc().b[13][1]++, null));
      /* istanbul ignore next */
      cov_lh93w0ojc().s[27]++;
      if (
      /* istanbul ignore next */
      (cov_lh93w0ojc().b[15][0]++, desc) && (
      /* istanbul ignore next */
      (cov_lh93w0ojc().b[15][1]++, desc.get) ||
      /* istanbul ignore next */
      (cov_lh93w0ojc().b[15][2]++, desc.set))) {
        /* istanbul ignore next */
        cov_lh93w0ojc().b[14][0]++;
        cov_lh93w0ojc().s[28]++;
        Object.defineProperty(newObj, key, desc);
      } else {
        /* istanbul ignore next */
        cov_lh93w0ojc().b[14][1]++;
        cov_lh93w0ojc().s[29]++;
        newObj[key] = obj[key];
      }
    } else
    /* istanbul ignore next */
    {
      cov_lh93w0ojc().b[11][1]++;
    }
  }
  /* istanbul ignore next */
  cov_lh93w0ojc().s[30]++;
  newObj.default = obj;
  /* istanbul ignore next */
  cov_lh93w0ojc().s[31]++;
  if (cache) {
    /* istanbul ignore next */
    cov_lh93w0ojc().b[16][0]++;
    cov_lh93w0ojc().s[32]++;
    cache.set(obj, newObj);
  } else
  /* istanbul ignore next */
  {
    cov_lh93w0ojc().b[16][1]++;
  }
  cov_lh93w0ojc().s[33]++;
  return newObj;
}
