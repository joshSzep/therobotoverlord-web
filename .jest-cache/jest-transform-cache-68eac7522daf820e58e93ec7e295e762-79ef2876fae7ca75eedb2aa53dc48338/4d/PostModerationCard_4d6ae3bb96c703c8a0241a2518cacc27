    /* istanbul ignore next */
    (cov_gisb0dc1c().b[24][0]++, action === 'reject') ||
    /* istanbul ignore next */
    (cov_gisb0dc1c().b[24][1]++, action === 'flag')) &&
    /* istanbul ignore next */
    (cov_gisb0dc1c().b[24][2]++, !reason.trim())) {
      /* istanbul ignore next */
      cov_gisb0dc1c().b[23][0]++;
      cov_gisb0dc1c().s[64]++;
      setShowReasonInput(action);
      /* istanbul ignore next */
      cov_gisb0dc1c().s[65]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_gisb0dc1c().b[23][1]++;
    }
    cov_gisb0dc1c().s[66]++;
    setIsSubmitting(true);
    /* istanbul ignore next */
    cov_gisb0dc1c().s[67]++;
    try {
      /* istanbul ignore next */
      cov_gisb0dc1c().s[68]++;
      await onModerate(action,
      /* istanbul ignore next */
      (cov_gisb0dc1c().b[25][0]++, reason.trim()) ||
      /* istanbul ignore next */
      (cov_gisb0dc1c().b[25][1]++, undefined));
      /* istanbul ignore next */
      cov_gisb0dc1c().s[69]++;
      setReason('');
      /* istanbul ignore next */
      cov_gisb0dc1c().s[70]++;
      setShowReasonInput(null);
    } finally {
      /* istanbul ignore next */
      cov_gisb0dc1c().s[71]++;
      setIsSubmitting(false);
    }
  };
  /* istanbul ignore next */
  cov_gisb0dc1c().s[72]++;
  const handleReasonSubmit = action => {
    /* istanbul ignore next */
    cov_gisb0dc1c().f[10]++;
    cov_gisb0dc1c().s[73]++;
    if (reason.trim()) {
      /* istanbul ignore next */
      cov_gisb0dc1c().b[26][0]++;
      cov_gisb0dc1c().s[74]++;
      handleModerationAction(action);
    } else
    /* istanbul ignore next */
    {
      cov_gisb0dc1c().b[26][1]++;
    }
  };
  /* istanbul ignore next */
  cov_gisb0dc1c().s[75]++;
  const handleReasonCancel = () => {
    /* istanbul ignore next */
    cov_gisb0dc1c().f[11]++;
    cov_gisb0dc1c().s[76]++;
