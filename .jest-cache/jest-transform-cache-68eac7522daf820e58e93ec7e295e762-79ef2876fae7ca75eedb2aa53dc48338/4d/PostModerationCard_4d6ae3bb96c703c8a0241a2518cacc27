                disabled:
                /* istanbul ignore next */
                (cov_gisb0dc1c().b[36][0]++, !reason.trim()) ||
                /* istanbul ignore next */
                (cov_gisb0dc1c().b[36][1]++, isSubmitting),
                className: showReasonInput === 'reject' ?
                /* istanbul ignore next */
                (cov_gisb0dc1c().b[37][0]++, 'bg-rejected-red hover:bg-rejected-red/80') :
