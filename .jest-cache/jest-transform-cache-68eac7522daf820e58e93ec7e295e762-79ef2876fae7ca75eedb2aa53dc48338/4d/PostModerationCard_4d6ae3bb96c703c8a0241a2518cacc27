    /* istanbul ignore next */
    cov_gisb0dc1c().s[77]++;
    setShowReasonInput(null);
  };
  /* istanbul ignore next */
  cov_gisb0dc1c().s[78]++;
  const truncateContent = (content, maxLength =
  /* istanbul ignore next */
  (cov_gisb0dc1c().b[27][0]++, 200)) => {
    /* istanbul ignore next */
    cov_gisb0dc1c().f[12]++;
    cov_gisb0dc1c().s[79]++;
