        cov_gisb0dc1c().s[54]++;
        return 'text-muted-light';
    }
  };
  /* istanbul ignore next */
  cov_gisb0dc1c().s[55]++;
  const getStatusIcon = status => {
    /* istanbul ignore next */
    cov_gisb0dc1c().f[8]++;
    cov_gisb0dc1c().s[56]++;
    switch (status) {
      case 'pending':
        /* istanbul ignore next */
        cov_gisb0dc1c().b[22][0]++;
        cov_gisb0dc1c().s[57]++;
