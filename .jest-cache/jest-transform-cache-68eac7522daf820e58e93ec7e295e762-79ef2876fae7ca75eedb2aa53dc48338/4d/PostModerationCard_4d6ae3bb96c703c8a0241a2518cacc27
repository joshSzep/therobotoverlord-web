      /* istanbul ignore next */
      cov_gisb0dc1c().b[28][0]++;
      cov_gisb0dc1c().s[80]++;
      return content;
    } else
    /* istanbul ignore next */
    {
      cov_gisb0dc1c().b[28][1]++;
    }
    cov_gisb0dc1c().s[81]++;
