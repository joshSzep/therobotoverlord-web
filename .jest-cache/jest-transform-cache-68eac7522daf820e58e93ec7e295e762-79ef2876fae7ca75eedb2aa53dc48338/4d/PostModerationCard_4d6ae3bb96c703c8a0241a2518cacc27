                /* istanbul ignore next */
                (cov_gisb0dc1c().b[30][0]++, post.topic) &&
                /* istanbul ignore next */
                (cov_gisb0dc1c().b[30][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
