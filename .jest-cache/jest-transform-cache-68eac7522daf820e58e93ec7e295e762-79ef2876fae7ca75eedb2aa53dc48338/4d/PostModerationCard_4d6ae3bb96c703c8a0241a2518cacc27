        /* istanbul ignore next */
        cov_gisb0dc1c().b[22][3]++;
        cov_gisb0dc1c().s[60]++;
        return '🚩';
      default:
        /* istanbul ignore next */
        cov_gisb0dc1c().b[22][4]++;
        cov_gisb0dc1c().s[61]++;
        return '❓';
    }
  };
  /* istanbul ignore next */
  cov_gisb0dc1c().s[62]++;
