  if (typeof WeakMap !== "function") {
    /* istanbul ignore next */
    cov_1tfm0scyck().b[0][0]++;
    cov_1tfm0scyck().s[9]++;
    return null;
  } else
  /* istanbul ignore next */
  {
    cov_1tfm0scyck().b[0][1]++;
  }
  var cacheBabelInterop =
  /* istanbul ignore next */
  (cov_1tfm0scyck().s[10]++, new WeakMap());
  var cacheNodeInterop =
  /* istanbul ignore next */
  (cov_1tfm0scyck().s[11]++, new WeakMap());
  /* istanbul ignore next */
  cov_1tfm0scyck().s[12]++;
  return (_getRequireWildcardCache = function (nodeInterop) {
    /* istanbul ignore next */
    cov_1tfm0scyck().f[2]++;
    cov_1tfm0scyck().s[13]++;
    return nodeInterop ?
    /* istanbul ignore next */
    (cov_1tfm0scyck().b[1][0]++, cacheNodeInterop) :
    /* istanbul ignore next */
    (cov_1tfm0scyck().b[1][1]++, cacheBabelInterop);
  })(nodeInterop);
}
function _interop_require_wildcard(obj, nodeInterop) {
  /* istanbul ignore next */
  cov_1tfm0scyck().f[3]++;
  cov_1tfm0scyck().s[14]++;
  if (
  /* istanbul ignore next */
  (cov_1tfm0scyck().b[3][0]++, !nodeInterop) &&
  /* istanbul ignore next */
  (cov_1tfm0scyck().b[3][1]++, obj) &&
  /* istanbul ignore next */
  (cov_1tfm0scyck().b[3][2]++, obj.__esModule)) {
    /* istanbul ignore next */
    cov_1tfm0scyck().b[2][0]++;
    cov_1tfm0scyck().s[15]++;
    return obj;
  } else
  /* istanbul ignore next */
  {
    cov_1tfm0scyck().b[2][1]++;
  }
  cov_1tfm0scyck().s[16]++;
  if (
  /* istanbul ignore next */
  (cov_1tfm0scyck().b[5][0]++, obj === null) ||
  /* istanbul ignore next */
  (cov_1tfm0scyck().b[5][1]++, typeof obj !== "object") &&
  /* istanbul ignore next */
  (cov_1tfm0scyck().b[5][2]++, typeof obj !== "function")) {
    /* istanbul ignore next */
    cov_1tfm0scyck().b[4][0]++;
    cov_1tfm0scyck().s[17]++;
    return {
      default: obj
    };
  } else
  /* istanbul ignore next */
  {
    cov_1tfm0scyck().b[4][1]++;
  }
  var cache =
  /* istanbul ignore next */
  (cov_1tfm0scyck().s[18]++, _getRequireWildcardCache(nodeInterop));
  /* istanbul ignore next */
  cov_1tfm0scyck().s[19]++;
  if (
  /* istanbul ignore next */
  (cov_1tfm0scyck().b[7][0]++, cache) &&
  /* istanbul ignore next */
  (cov_1tfm0scyck().b[7][1]++, cache.has(obj))) {
    /* istanbul ignore next */
    cov_1tfm0scyck().b[6][0]++;
    cov_1tfm0scyck().s[20]++;
    return cache.get(obj);
  } else
  /* istanbul ignore next */
  {
    cov_1tfm0scyck().b[6][1]++;
  }
  var newObj =
  /* istanbul ignore next */
  (cov_1tfm0scyck().s[21]++, {
    __proto__: null
  });
  var hasPropertyDescriptor =
  /* istanbul ignore next */
  (cov_1tfm0scyck().s[22]++,
  /* istanbul ignore next */
  (cov_1tfm0scyck().b[8][0]++, Object.defineProperty) &&
  /* istanbul ignore next */
  (cov_1tfm0scyck().b[8][1]++, Object.getOwnPropertyDescriptor));
  /* istanbul ignore next */
  cov_1tfm0scyck().s[23]++;
  for (var key in obj) {
    /* istanbul ignore next */
    cov_1tfm0scyck().s[24]++;
    if (
    /* istanbul ignore next */
    (cov_1tfm0scyck().b[10][0]++, key !== "default") &&
    /* istanbul ignore next */
    (cov_1tfm0scyck().b[10][1]++, Object.prototype.hasOwnProperty.call(obj, key))) {
      /* istanbul ignore next */
      cov_1tfm0scyck().b[9][0]++;
      var desc =
      /* istanbul ignore next */
      (cov_1tfm0scyck().s[25]++, hasPropertyDescriptor ?
      /* istanbul ignore next */
      (cov_1tfm0scyck().b[11][0]++, Object.getOwnPropertyDescriptor(obj, key)) :
      /* istanbul ignore next */
      (cov_1tfm0scyck().b[11][1]++, null));
      /* istanbul ignore next */
      cov_1tfm0scyck().s[26]++;
      if (
      /* istanbul ignore next */
      (cov_1tfm0scyck().b[13][0]++, desc) && (
      /* istanbul ignore next */
      (cov_1tfm0scyck().b[13][1]++, desc.get) ||
      /* istanbul ignore next */
      (cov_1tfm0scyck().b[13][2]++, desc.set))) {
        /* istanbul ignore next */
        cov_1tfm0scyck().b[12][0]++;
        cov_1tfm0scyck().s[27]++;
        Object.defineProperty(newObj, key, desc);
      } else {
        /* istanbul ignore next */
        cov_1tfm0scyck().b[12][1]++;
        cov_1tfm0scyck().s[28]++;
        newObj[key] = obj[key];
      }
    } else
    /* istanbul ignore next */
    {
      cov_1tfm0scyck().b[9][1]++;
    }
  }
  /* istanbul ignore next */
  cov_1tfm0scyck().s[29]++;
  newObj.default = obj;
  /* istanbul ignore next */
  cov_1tfm0scyck().s[30]++;
  if (cache) {
    /* istanbul ignore next */
    cov_1tfm0scyck().b[14][0]++;
    cov_1tfm0scyck().s[31]++;
    cache.set(obj, newObj);
  } else
  /* istanbul ignore next */
  {
    cov_1tfm0scyck().b[14][1]++;
  }
  cov_1tfm0scyck().s[32]++;
  return newObj;
}
function PostForm({
  onSubmit,
  onCancel,
  initialData,
