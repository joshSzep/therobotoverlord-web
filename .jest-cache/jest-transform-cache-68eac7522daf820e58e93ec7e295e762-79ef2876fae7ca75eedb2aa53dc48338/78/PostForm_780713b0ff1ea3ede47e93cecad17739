          }))]
        })), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
          className: "flex items-center justify-end space-x-3 pt-4 border-t border-muted/20",
          children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_Button.Button, {
            type: "button",
            variant: "ghost",
            onClick: onCancel,
            disabled: isSubmitting,
            children: "Cancel"
          }), /*#__PURE__*/(0, _jsxruntime.jsxs)(_Button.Button, {
            type: "submit",
            variant: "primary",
            disabled:
            /* istanbul ignore next */
            (cov_1tfm0scyck().b[42][0]++, isSubmitting) ||
            /* istanbul ignore next */
            (cov_1tfm0scyck().b[42][1]++, !formData.content.trim()),
            className: "flex items-center space-x-2",
            children: [
            /* istanbul ignore next */
            (cov_1tfm0scyck().b[43][0]++, isSubmitting) &&
            /* istanbul ignore next */
            (cov_1tfm0scyck().b[43][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
              className: "w-4 h-4 border-2 border-white/30 border-t-white rounded-full animate-spin"
            })), /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
              children: isSubmitting ?
              /* istanbul ignore next */
              (cov_1tfm0scyck().b[44][0]++, 'Submitting...') :
              /* istanbul ignore next */
              (cov_1tfm0scyck().b[44][1]++, mode === 'edit' ?
              /* istanbul ignore next */
              (cov_1tfm0scyck().b[45][0]++, 'Update Post') :
              /* istanbul ignore next */
              (cov_1tfm0scyck().b[45][1]++, mode === 'reply' ?
              /* istanbul ignore next */
              (cov_1tfm0scyck().b[46][0]++, 'Post Reply') :
              /* istanbul ignore next */
              (cov_1tfm0scyck().b[46][1]++, 'Create Post')))
            })]
          })]
        })]
      })
    })]
  });
}
